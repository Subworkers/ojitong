    "        keywords = [\"지연/사고 일시\", \"지연/사고 노선\", \"지연/사고 이유\"***REMOVED***\n",
    "    elif category == \"파업 \":\n",
    "        keywords = [\"파업 일시\", \"파업 노선\", \"파업 이유\"***REMOVED***\n",
